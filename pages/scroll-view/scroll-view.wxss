page{
    width:100%;
    height: 100%;
}
.container{
    width:100%;
    height:100%;
    font-size: 14px;
}
.nav{
    width: 100%;
    height: 40px;
    background: olivedrab;
    position: fixed;
    top:0;
    z-index: 1;
    white-space: nowrap;
}
.nav-title{
    display: inline-block;
    width: 20%;
    text-align: center;
    height: 100%;
    line-height: 40px;
    margin-left: 20px;
    color: #fff;
}

.carousel{
    width: 100%;
    height: 150px;
    position: fixed;
    top:40px;
    z-index: 1;
    white-space: nowrap;
}
.carousel-img{
    display: inline-block;
    height: 100%;
    width: 100%;
}

